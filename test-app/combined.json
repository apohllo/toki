{
    "input layer":
    {
        "buffer": "2000"
    },
    "layer_split":
    {
        "ignore_types": "t_ip",
        "class": "split",
        "separators": "/!.",
        "sep_tok_type": "@"
    },
    "layer_combine":
    {
        "class": "combine",
        "cutoff": "0.1"
    },
    "layer_append_plus":
    {
        "class": "append",
        "do_not_process_types": "a,b",
        "append": "+?"
    },
    "layer_append_minus":
    {
        "class": "append",
        "process_types": "t",
        "append": "-"
    },
    "layer_affix":
    {
        "class": "affix_split",
        "prefix_chars": "(",
        "postfix_chars": "\")",
        "prefix_tok_type": "a",
        "postfix_tok_type": "b"
    },
    "layer_classify":
    {
        "class": "classify_regexp",
        "type_szatan": "szatan",
        "type_ogonki": "[\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF\uFFFF]*",
        "type_ip": "([0-9]{1,3})(.[0-9]{1,3}){3}"
    },
    "layers":
    {
        "x": "affix",
        "z": "classify",
        "a": "append_plus",
        "b": "append_minus"
    },
    "input":
    {
        "token_type": "t",
        "z": "split"
    },
    "layer":
    {
        "id": "pre_split",
        "cutoff": "0.2",
        "separators": "\"my_important.txt\",@,\"catch_all.txt\""
    },
    "layer_my_split":
    {
        "clone": "split"
    }
}
